<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="shoppingCart" match-name=".*"/>
  <table-filter match-catalog="shoppingCart" match-name="address"/>
  <table-filter match-catalog="shoppingCart" match-name="cart"/>
  <table-filter match-catalog="shoppingCart" match-name="cart_line"/>
  <table-filter match-catalog="shoppingCart" match-name="category"/>
  <table-filter match-catalog="shoppingCart" match-name="order_detail"/>
  <table-filter match-catalog="shoppingCart" match-name="order_item"/>
  <table-filter match-catalog="shoppingCart" match-name="product"/>
  <table-filter match-catalog="shoppingCart" match-name="user_detail"/>
</hibernate-reverse-engineering>